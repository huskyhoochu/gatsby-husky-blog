{
  "name": "husky-blog",
  "description": "Huskyhoochu's tech blog",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "prettier": "prettier --single-quote --arrow-parens always --write",
    "lint": "eslint --fix",
    "removeCache": "rm -rf .cache public",
    "removeUrl": "now rm -y",
    "deploy": "./scripts/deploy.sh"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ./scripts/image_optimization.sh $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./scripts/add_issue_number_into_commit.sh $HUSKY_GIT_PARAMS",
      "post-merge": "./scripts/deploy.sh"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "gatsby": "^2.0.52",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-offline": "^2.0.15",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.2",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.12",
    "gatsby-transformer-sharp": "^2.1.8",
    "lodash": "^4.17.11",
    "minireset.css": "^0.0.3",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-adsense": "^0.0.6",
    "react-dom": "^16.6.3",
    "react-facebook": "^6.0.15",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2"
  }
}
